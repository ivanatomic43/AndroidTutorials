<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.managesmsapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ManageSMSApp">
        <activity android:name=".activity.Runtime"></activity>
        <activity android:name=".activity.ListView" />
        <activity android:name=".activity.GridLayout" />
        <activity android:name=".activity.TableLayout" />
        <activity android:name=".activity.LinearLayout" />
        <activity android:name=".activity.LayoutActivity" />
        <activity android:name=".activity.LoginActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>